<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/spacex.css">
</head>
<body >
    <div total>
    <i>
        <div class="navbar">
         <div class="links">
             <a href="home" id="home">Home</a>
                 <a href="about" id="about">About</a>
                 <a href="services" id="service">Services</a>
                 <a href="contact" id=contact>Contact</a>
         </div>
        </div>
   
    <h1>SpaceX crew members are: </h1>
    <div class="grid-container">
    <% for( let i = 0; i < data.length; i++ ) { %>
    
           <div id="border"> <img src="<%= data[i].image %>" alt="Image not found" height="400px" width="400px" class="grid-item">
            <h3>Name: <a href="<%= data[i].wikipedia %>"><%= data[i].name %></a></h3>
            <p>Agency: <%= data[i].agency %></p>
        </div>
        
    <% } %></div>
</div>
</body>
</html>
