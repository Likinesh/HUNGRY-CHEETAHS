<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/spacex.css">
</head>
<body>
    <i>
        <div class="navbar">
         <div class="links">
             <a href="home" id="home">Home</a>
                 <a href="about" id="about">About</a>
                 <a href="services" id="service">Services</a>
                 <a href="contact" id=contact>Contact</a>
         </div>
        </div>
    <h1>SpaceX Landpads are: </h1>
    <% for( let i = 0; i < data.length; i++ ) { %>
        <div id="border">
        <a id="main" href="<%= data[i].wikipedia %>"><%= data[i].name %></a> 
        <div id="img"><img src="<%= data[i].images.large[0] %>" alt="Image not found" height="400px" width="500px"></div>
        <p>Full name: <%= data[i].full_name %></p>
        <p>Status: <%= data[i].status %></p>
        <p>Type: <%= data[i].type %></p>
        <p>Region: <%= data[i].locality %>,<%= data[i].region %></p>
        <p>Details: <%= data[i].details %></p>
    </div>
    <% } %>

</body>
</html>
