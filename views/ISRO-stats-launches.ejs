<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css" integrity="sha512-doJrC/ocU8VGVRx3O9981+2aYUn3fuWVWvqLi1U+tA2MWVzsw+NVKq1PrENF03M+TYBP92PnYUlXFH1ZW0FpLw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="css/table.css">
</head>
<body style="background-image:url('pictures/bckgd.jpg');">
  <div class="navbar">
    <div class="links" >
        <a href="home" id="home">Home</a>
        <a href="about" id="about">About</a>
        <a href="services" id="service">Services</a>
        <a href="contact" id=contact>Contact</a>
    </div>
</div>
  
    <h1 style="color: wheat;">ISRO Launches are:</h1>
    <table style="background-image:url('pictures/table4.jpeg');">
        <tr id="table" style="background-color: aqua;">
        <!-- <th>Sl.no</th> -->
        <th>Name</th>
        <th>Launch Date</th>
        <th>Launch Type</th>
        <th>Payload</th>
        <th>Status</th>
        </tr>
            <% for(var i=0; i<data.length; i++) { %>
                <tr>
                <!-- <td> <%= data[i].SerialNumber %></td> -->
                <td> <a style= "a.hover{color:black}" href="<%= data[i].Link %>"><%= data[i].Name %></a></td>
                <td style="color:black"> <%= data[i].LaunchDate %></td>
                <td style="color:black;"> <%= data[i].LaunchType %></td>
                <td style="color:black;"> <%= data[i].Payload %></td>
                <td style="color:black;"> <%= data[i].MissionStatus %></td>
                </tr>
            <% } %>
    </table>
</body>
</html>