    <h1>StarLinks launched till date:</h1>
    <div class="grid-container">
    <% for( let i = num-1; i>=num-300; i-- ) { %>
        <div id="border">
        <h3>Name: <%= data[i].spaceTrack.OBJECT_NAME %></h3>
        <p>Launched on: <%= data[i].spaceTrack.LAUNCH_DATE %></p>
        <p>Launched by: <%= data[i].spaceTrack.COUNTRY_CODE %></p>
       <% if ( data[i].spaceTrack.DECAY_DATE !== null) { %>
        <p> Decayed on: <%= data[i].spaceTrack.DECAY_DATE %>
       <% } %>
       <% if ( data[i].spaceTrack.DECAY_DATE === null) { %>
        <p> Velocity: <%= data[i].velocity_kms %>kmph
       <% } %></p>
        <p>Location: <%= data[i].latitude %>,<%= data[0].longitude %></p>
    </div>

    <% } %>
</div>
<% if (num!=3526) { %>
    <form action="/starlink-prev" method="post">
        <button type="submit" id="prev">Previous</button>
    </form>
<% } %>
<% if (num!=300) { %>
    <form action="/starlink-next" method="post">
        <button type="submit" id="next">Next</button>
    </form>
<% } %>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
