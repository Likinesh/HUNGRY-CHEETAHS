<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/spacex.css">
</head>
<body>
    <i>
        <div class="navbar">
         <div class="links">
             <a href="home" id="home">Home</a>
                 <a href="about" id="about">About</a>
                 <a href="services" id="service">Services</a>
                 <a href="contact" id=contact>Contact</a>
         </div>
        </div>
     <div id="heading"><h1>Spacex History</h1></div>
    <% for( let i = data.length-1; i>=0; i-- ) { %>
        <div id="border">
        <h3><a id="title">Title: </a><a href="<%= data[i].links.article %>"><%= data[i].title %></a></h3>
        <p> <a id="event">Event Date: </a><%= data[i].event_date_utc  %></p>
        <p> <a id="details">Details: </a><%= data[i].details  %></p>
    </div>
    <% } %>
</body>
</html>
