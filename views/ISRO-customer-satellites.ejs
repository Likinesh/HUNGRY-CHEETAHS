<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/table.css">
</head>
<body>
    <body style="background-image:url('pictures/bckgd.jpg');">
        <div class="navbar">
          <div class="links" >
              <a href="home" id="home">Home</a>
              <a href="about" id="about">About</a>
              <a href="services" id="service">Services</a>
              <a href="contact" id=contact>Contact</a>
          </div>
      </div>
    <h1 style="color: wheat;">Customer Satellites Launched are</h1>
    <table style="background-image:url('pictures/table4.jpeg');">
        <tr id="table" style="background-color: aqua;">
        <th>Name of Satellite</th>
        <th>Launch Date</th>
        <th>Launch by</th>
        <th>Mass</th>
        <th>Launcher</th>
        </tr>
            <% for(var i=0; i<data1.customer_satellites.length; i++) { %>
                <tr>
                <td><%= data1.customer_satellites[i].id %></td>
                <td> <%= data1.customer_satellites[i].launch_date %></td>
                <td> <%= data1.customer_satellites[i].country %></td>
                <td> <%= data1.customer_satellites[i].mass %> kg</td>
                <td> <%= data1.customer_satellites[i].launcher %></td>
                </tr>
            <% } %>
    </table>
</body>
</html>